{
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "build:staging": "NEXT_PUBLIC_API_ENV=staging next build",
    "build:prod": "cross-env NEXT_PUBLIC_API_ENV=production next build",
    "start:staging": "NEXT_PUBLIC_API_ENV=staging next start",
    "start:prod": "NEXT_PUBLIC_API_ENV=production next start",
    "start": "next start",
    "eslint:check": "eslint  --ext .jsx,.js,.ts,.tsx src/ --fix",
    "format": "prettier \"./src/**/*.{js,jsx,ts,tsx}\" --write",
    "test": "cross-env NODE_ENV=test jest",
    "test:dev": "cross-env NODE_ENV=test jest --watchAll",
    "coverage:jest": "cross-env NODE_ENV=test jest --coverage",
    "coverage:cypress": "cross-env NODE_ENV=test cypress run",
    "cypress:open": "cross-env NODE_ENV=test cypress open",
    "test:coverage": "cross-env NODE_ENV=test CODE_COVERAGE=true yarn coverage:cypress && node ./scripts/mergeCoverage.js",
    "test:ci": "cross-env NODE_ENV=test yarn coverage:delete && yarn coverage:jest && start-server-and-test test:staging http://localhost:3000 test:coverage",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "dependencies": {
    "@hookform/resolvers": "^2.9.11",
    "axios": "^1.3.4",
    "next": "latest",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-hook-form": "^7.43.2",
    "yup": "^1.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.21.0",
    "@next/eslint-plugin-next": "^13.2.1",
    "@storybook/addon-actions": "^6.5.16",
    "@storybook/addon-essentials": "^6.5.16",
    "@storybook/addon-interactions": "^6.5.16",
    "@storybook/addon-links": "^6.5.16",
    "@storybook/addon-postcss": "^2.0.0",
    "@storybook/builder-webpack5": "^6.5.16",
    "@storybook/manager-webpack5": "^6.5.16",
    "@storybook/react": "^6.5.16",
    "@storybook/testing-library": "^0.0.13",
    "@types/node": "18.11.3",
    "@types/react": "18.0.21",
    "@types/react-dom": "18.0.6",
    "@typescript-eslint/eslint-plugin": "^5.53.0",
    "@typescript-eslint/parser": "^5.53.0",
    "autoprefixer": "^10.4.13",
    "babel-loader": "^8.3.0",
    "cypress": "^12.7.0",
    "eslint": "^8.35.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-standard": "^17.0.0",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-standard": "^5.0.0",
    "jest": "^29.4.3",
    "postcss": "^8.4.21",
    "postcss-loader": "^7.0.2",
    "tailwindcss": "^3.2.4",
    "typescript": "4.9.4"
  },
  "resolutions": {
    "webpack": "^5"
  }
}
